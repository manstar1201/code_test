    "def combine(n, k):\n",
    "    result = []\n",
    "    cont = []\n",
    "    \n",
    "    def dfs(t, cont):\n",
    "        \n",
    "        if len(cont) == k:\n",
    "            result.append(cont[:])\n",
    "            return\n",
    "        \n",
    "        for i in range(t, n+1):\n",
    "            cont.append(i)\n",
    "            p = i + 1\n",
    "            dfs(p, cont)\n",
    "            cont.pop()\n",
    "            \n",
    "    dfs(1, cont)\n",
    "    \n",
    "    return result"
   ]
