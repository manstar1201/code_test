    "from collections import defaultdict\n",
    "def findItinerary(tickets: list):\n",
    "    mapping = defaultdict(list)\n",
    "    result = []\n",
    "    \n",
    "    for depart, arrive in sorted(tickets):\n",
    "        mapping[depart].append(arrive)\n",
    "        \n",
    "    def dfs(start):\n",
    "        \n",
    "        while mapping[start]:\n",
    "            dfs(mapping[start].pop(0))\n",
    "            \n",
    "        result.append(start)\n",
    "        \n",
    "    dfs('JFK')\n",
    "    \n",
    "    return result[::-1]"
   ]
