    "def productExceptSelf(self, nums):\n",
    "    out = []\n",
    "    \n",
    "    p = 1\n",
    "    for i in range(len(nums)):\n",
    "        out.append(p)\n",
    "        p = p * nums[i]\n",
    "        \n",
    "    p = 1\n",
    "    for i in range(len(nums)-1, -1, -1):\n",
