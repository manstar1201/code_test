    "def permute(nums: list):\n",
    "    result = []\n",
    "    cont = []\n",
    "    \n",
    "    def dfs(x, cont):\n",
    "        if len(x) == 0:\n",
    "            result.append(cont[:])\n",
    "            # cont의 값을 복사해서 append해야함. 왜냐하면 cont를 append할 시 수정된\n",
    "            # cont가 append 될 수도 있음\n",
    "            return\n",
    "        \n",
    "        for i in range(len(x)):\n",
    "            temp = x[:]\n",
    "            cont += [temp.pop(i)]\n",
    "            dfs(temp, cont)\n",
    "            cont.pop()\n",
    "            #이전 단계의 cont로 돌아가기 위해서는 반드시 cont의 마지막 값을 pop해야함\n",
    "            \n",
    "    dfs(nums, cont)\n",
    "        \n",
    "    return result"
   ]
  },
