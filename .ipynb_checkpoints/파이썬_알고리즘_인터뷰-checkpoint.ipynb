    "def isValid(s: str):\n",
    "    stack = []\n",
    "    mapping = {')' : '(', ']' : '[', '}' : '{'}\n",
    "    \n",
    "    for c in s:\n",
    "        # dict 구조에서는 key값을 in 으로 판단 가능\n",
    "        if c in mapping:\n",
    "            if stack and stack[-1] == mapping[c]:\n",
    "                stack.pop()\n",
    "            else:\n",
    "                return False\n",
    "        else:\n",
    "            stack.append(c)\n",
    "        \n",
    "    return True if not stack else False"
