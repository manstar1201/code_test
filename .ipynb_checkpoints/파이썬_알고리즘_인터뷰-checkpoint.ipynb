    "def swapPairs(self, head):\n",
    "    dummy = ListNode(0, head)\n",
    "    prev, curr = dummy, head\n",
    "        \n",
    "    while curr and curr.next:\n",
    "        #save ptrs\n",
    "        nextpair = curr.next.next\n",
    "        second = curr.next\n",
    "            \n",
    "        #swap nodes\n",
    "        second.next = curr\n",
    "        curr.next = nextpair\n",
    "        prev.next = second\n",
    "            \n",
    "        #change ptrs\n",
    "        prev = curr\n",
    "        curr = nextpair\n",
    "        \n",
    "    return dummy.next"
   ]
