    "def letterCombinations(self, digits: str) -> List[str]:\n",
    "    res = []\n",
    "    digitToChar = { \"2\": \"abc\",\n",
    "                   \"3\": \"def\",\n",
    "                   \"4\": \"ghi\",\n",
    "                   \"5\": \"jkl\",\n",
    "                   \"6\": \"mno\",\n",
    "                   \"7\": \"qprs\",\n",
    "                   \"8\": \"tuv\",\n",
    "                   \"9\": \"wxyz\" }\n",
    "        \n",
    "    def backtrack(i, curStr):\n",
    "        if len(curStr) == len(digits):\n",
    "            res.append(curStr)\n",
    "            return\n",
    "        for c in digitToChar[digits[i]]:\n",
    "            backtrack(i + 1, curStr + c)\n",
    "        \n",
    "    if digits:\n",
    "        backtrack(0, \"\")\n",
    "        \n",
    "    return res"
   ]
