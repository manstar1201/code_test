    "def addTwoNumbers(l1, l2):\n",
    "    dummy = ListNode()\n",
    "    tail = dummy\n",
    "    \n",
    "    carry = 0\n",
    "    \n",
    "    while l1 or l2 or carry:\n",
    "        summ = 0 \n",
    "        if l1:\n",
    "            summ += l1.val\n",
    "            l1 = l1.next\n",
    "        elif l2:\n",
    "            summ += l2.val\n",
    "            l2 = l2.next\n",
    "        \n",
    "        carry, value = divmod(summ + carry, 10)\n",
    "        tail.next = ListNode(value)\n",
    "        tail = tail.next\n",
    "    \n",
    "    return dummy.next"
   ]
