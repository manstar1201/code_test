    "def removeDuplicateLetters(s: str):\n",
    "    stack = []\n",
    "    \n",
    "    for idx, c in enumerate(s):\n",
    "        if not stack:\n",
    "            stack.append(c)\n",
    "            continue\n",
    "        \n",
    "        elif c in stack:\n",
    "            continue\n",
    "        \n",
    "        while stack and (stack[-1] in s[idx:]) and (stack[-1] > c):\n",
    "            stack.pop()\n",
    "        \n",
    "        stack.append(c)\n",
    "        \n",
    "    return ''.join(stack)"
   ]
